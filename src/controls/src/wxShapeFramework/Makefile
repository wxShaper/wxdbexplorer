ifndef CONFIG
  CONFIG=Release
endif

# if multiple archs are defined turn off automated dependency generation
DEPFLAGS := $(if $(word 2, $(TARGET_ARCH)), , -MMD)

ifeq ($(CONFIG),Release)
  BINDIR := ../lib/gcc_dll
  LIBDIR := ../build
  OBJDIR := .objsu
  OUTDIR := ../lib/gcc_dll
  CPPFLAGS := $(DEPFLAGS) -D "WXMAKINGDLL_WXSF" -D "WXMAKINGDLL_WXXS" -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "NDEBUG" -I "../include" -I "../src"
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -fPIC -O3 -Wall -fno-strict-aliasing `wx-config --debug=no --static=no --cflags`
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -shared -s `wx-config --debug=no --static=no --libs`
  LDDEPS :=
  RESFLAGS := -D "WXMAKINGDLL_WXSF" -D "WXMAKINGDLL_WXXS" -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "NDEBUG" -I "../include" -I "../src"
  TARGET := libwx_gtk2u_wxsf-2.8.so
 BLDCMD = $(CXX) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
endif

ifeq ($(CONFIG),Debug)
  BINDIR := ../lib/gcc_dll
  LIBDIR := ../build
  OBJDIR := .objsud
  OUTDIR := ../lib/gcc_dll
  CPPFLAGS := $(DEPFLAGS) -D "WXMAKINGDLL_WXSF" -D "WXMAKINGDLL_WXXS" -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "DEBUG" -D "_DEBUG" -D "__WXDEBUG__" -I "../include" -I "../src"
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -fPIC -g -Wall -O0 `wx-config --debug=yes --static=no --cflags`
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -shared `wx-config --debug=yes --static=no --libs`
  LDDEPS :=
  RESFLAGS := -D "WXMAKINGDLL_WXSF" -D "WXMAKINGDLL_WXXS" -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "DEBUG" -D "_DEBUG" -D "__WXDEBUG__" -I "../include" -I "../src"
  TARGET := libwx_gtk2ud_wxsf-2.8.so
 BLDCMD = $(CXX) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
endif

