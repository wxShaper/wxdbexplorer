ifndef CONFIG
  CONFIG=Release
endif

# if multiple archs are defined turn off automated dependency generation
DEPFLAGS := $(if $(word 2, $(TARGET_ARCH)), , -MMD)

ifeq ($(CONFIG),Release)
  BINDIR := ../bin/gcc
  LIBDIR := ../bin/gcc
  OBJDIR := .objsu
  OUTDIR := ../bin/gcc
  CPPFLAGS := $(DEPFLAGS) -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "NDEBUG" -I "controls/include" -I "."
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -O3 -Wall -fno-strict-aliasing `wx-config --debug=no --cflags`
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -s -Wl,-rpath,$$``ORIGIN/lib/ `wx-config --libs` -L../bin/gcc/lib -lwx_gtk2u_propgrid-2.8_cd -L../bin/gcc/lib -lwx_gtk2u_scintilla-2.8_cd -L../bin/gcc/lib -lwx_gtk2u_shapeframework-2.8_cd -lwxcode_gtk2_databaselayer_mysql-2.8 -lwxcode_gtk2_databaselayer_sqlite-2.8
  LDDEPS := ../bin/gcc/lib/libwx_gtk2u_propgrid-2.8_cd.so ../bin/gcc/lib/libwx_gtk2u_scintilla-2.8_cd.so ../bin/gcc/lib/libwx_gtk2u_shapeframework-2.8_cd.so
  RESFLAGS := -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "NDEBUG" -I "controls/include" -I "."
  TARGET := DatabaseExplorer
 BLDCMD = $(CXX) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)
endif

ifeq ($(CONFIG),Debug)
  BINDIR := ../bin/gcc
  LIBDIR := ../bin/gcc
  OBJDIR := .objsud
  OUTDIR := ../bin/gcc
  CPPFLAGS := $(DEPFLAGS) -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "DEBUG" -D "_DEBUG" -D "__WXDEBUG__" -I "controls/include" -I "."
  CFLAGS += $(CPPFLAGS) $(TARGET_ARCH) -g -Wall -O0 `wx-config --debug=yes --cflags`
  CXXFLAGS += $(CFLAGS)
  LDFLAGS += -L$(BINDIR) -L$(LIBDIR) -Wl,-rpath,$$``ORIGIN/lib/ `wx-config --debug=yes --libs` -L../bin/gcc/lib -lwx_gtk2ud_propgrid-2.8_cd -L../bin/gcc/lib -lwx_gtk2ud_scintilla-2.8_cd -L../bin/gcc/lib -lwx_gtk2ud_shapeframework-2.8_cd -lwxcode_gtk2_databaselayer_mysql-2.8 -lwxcode_gtk2_databaselayer_sqlite-2.8
  LDDEPS := ../bin/gcc/lib/libwx_gtk2ud_propgrid-2.8_cd.so ../bin/gcc/lib/libwx_gtk2ud_scintilla-2.8_cd.so ../bin/gcc/lib/libwx_gtk2ud_shapeframework-2.8_cd.so
  RESFLAGS := -D "WXUSINGDLL" -D "UNICODE" -D "_UNICODE" -D "__WX__" -D "DEBUG" -D "_DEBUG" -D "__WXDEBUG__" -I "controls/include" -I "."
  TARGET := DatabaseExplorer_d
 BLDCMD = $(CXX) -o $(OUTDIR)/$(TARGET) $(OBJECTS) $(LDFLAGS) $(RESOURCES) $(TARGET_ARCH)

  define prebuildcmd
	@echo Running pre-build commands...
	python $(ProjectPath)/create_buildnum.py
  endef
endif

OBJECTS := \
	$(OBJDIR)/column.o \
	$(OBJDIR)/columncol.o \
	$(OBJDIR)/tablecol.o \
	$(OBJDIR)/DatabaseExplorerApp.o \
	$(OBJDIR)/DbTable.o \
